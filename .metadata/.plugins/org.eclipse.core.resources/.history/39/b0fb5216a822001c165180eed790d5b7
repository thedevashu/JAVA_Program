
public class StackClass {
	private int stk[];
	private int top=-1;
	public StackClass(int size) {
		stk = new int[size];
		
	}
	
	
	public void push(int num) {
		
			top++;
			stk[top]=num;
			System.out.println(stk[top]);
		
	}
	
	public int pop() {
		top--;
		for (int i = 0; i < top; i++) {
			
			System.out.print(stk[i]);
		}
		if(top==0)
			System.out.println("Are remainig ek]elment");
		return stk[top];
	}
}

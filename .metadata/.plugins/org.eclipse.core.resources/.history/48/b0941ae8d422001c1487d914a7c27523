
public class StackClass {
	private int stk[];
	private int top=-1;
	public StackClass(int size) {
		stk = new int[size];

	}


	public void push(int num) {

		top++;
		stk[top]=num;
		System.out.println(stk[top]);

	}

	public void pop() {
		
		if(top!=-1)
		{
			top--;
		}
		
		else
			System.out.println("Stack is empty");

	}
}
